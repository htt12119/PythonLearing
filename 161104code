#-*- coding:utf-8 -*-
import re
m = re.search('(?<=abc)def','abcdef')
print "re模块的示例代码输出结果：",m.group(0)
#re 模块的示例代码

def abs(n):
    '''
    Function to get absolute value of number.

    Example:
    >>>abs(1)
    1
    >>>abs(-1)
    1
    >>>abs(0)
    0
    '''

    return n if n>= 0 else (-n)
print abs(1)



